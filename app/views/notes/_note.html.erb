<div id="<%= dom_id note %>" class='note border-bottom'>
  <p>
    <strong>Title:</strong>
    <%= note.title %>
  </p>

  <p>
    <strong>Body:</strong>
    <%= note.body %>
  </p>

  <p>
    <strong>Project:</strong>
    <%= note.project.title %>
  </p>

  <p>
    <strong>Rank:</strong>
    <%= note.rank %>
  </p>

  <p>
    <%= link_to 'show this note', note_path(note), target: '_top' %>
  </p>

</div>
